# Target Config
CONFIG_TARGET_x86=y
CONFIG_TARGET_x86_64=y
CONFIG_TARGET_x86_64_DEVICE_generic=y

# Set Firmware Size
CONFIG_TARGET_KERNEL_PARTSIZE=16
CONFIG_TARGET_ROOTFS_PARTSIZE=400

# Firmware Type（只保留常用的SquashFS格式，避免多格式冲突）
# CONFIG_GRUB_CONSOLE is not set
CONFIG_GRUB_IMAGES=y
CONFIG_GRUB_EFI_IMAGES=y
CONFIG_TARGET_IMAGES_GZIP=y
# CONFIG_TARGET_ROOTFS_EXT4FS is not set
CONFIG_TARGET_ROOTFS_SQUASHFS=y
# CONFIG_TARGET_ROOTFS_TARGZ is not set  # 注释掉，避免根文件系统格式冲突
# CONFIG_VMDK_IMAGES is not set

# Applications（补全luci-app对应的服务依赖包）
# CONFIG_PACKAGE_luci-app-accesscontrol is not set
CONFIG_PACKAGE_adbyby-plus=y  # 补全adbyby-plus服务包（luci-app-adbyby-plus依赖）
CONFIG_PACKAGE_luci-app-adbyby-plus=y
CONFIG_PACKAGE_adguardhome=y  # 补全adguardhome服务包（luci-app-adguardhome依赖）
CONFIG_PACKAGE_luci-app-adguardhome=y
CONFIG_PACKAGE_openlist=y  # 补全openlist服务包（推测依赖）
CONFIG_PACKAGE_luci-app-openlist=y
CONFIG_PACKAGE_luci-app-argon-config=y
CONFIG_PACKAGE_arpbind=y  # 补全arpbind服务包
CONFIG_PACKAGE_luci-app-arpbind=y
CONFIG_PACKAGE_autoreboot=y  # 补全autoreboot服务包
CONFIG_PACKAGE_luci-app-autoreboot=y
CONFIG_PACKAGE_ddns-scripts=y  # 补全ddns基础服务包
